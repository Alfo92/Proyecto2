<p id="notice"><%= notice %></p>

<h1>Listing Saved Listings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Listing</th>
      <th>Sort order</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_listings.each do |saved_listing| %>
      <tr>
        <td><%= saved_listing.user_id %></td>
        <td><%= saved_listing.listing_id %></td>
        <td><%= saved_listing.sort_order %></td>
        <td><%= saved_listing.notes %></td>
        <td><%= link_to 'Show', saved_listing %></td>
        <td><%= link_to 'Edit', edit_saved_listing_path(saved_listing) %></td>
        <td><%= link_to 'Destroy', saved_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved listing', new_saved_listing_path %>
