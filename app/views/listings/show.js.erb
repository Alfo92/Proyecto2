// summaries
<% general_rate = @listing.property_general_rate %>
$(".property-summ small").html('(<%= "#{t('listings.rate.average')}: #{general_rate.round(2)}" %> <i class="fa fa-star-o"></i>)');
$(".general-property-star").html('<%= j (rating_for @listing, "general_property", enable_half: true, half_show: true, readonly: true, noRatedMsg: t('listings.rate.general_property'), targetScore: general_rate) %>');
initOneStar($(".general-property-star .star"));

<% general_rate = @listing.listing_general_rate %>
$(".listing-summ small").html('(<%= "#{t('listings.rate.average')}: #{general_rate.round(2)}" %> <i class="fa fa-star-o"></i>)');
$(".general-listing-star").html('<%= j (rating_for @listing, "general_listing", enable_half: true, half_show: true, readonly: true, noRatedMsg: t('listings.rate.general_listing'), targetScore: general_rate) %>');
initOneStar($(".general-listing-star .star"));